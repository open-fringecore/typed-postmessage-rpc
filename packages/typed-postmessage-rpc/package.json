{
    "name": "typed-postmessage-rpc",
    "version": "1.2.1",
    "description": "Strongly typed RPC framework inspired by tRPC for web workers, iframes & windows.",
    "exports": {
        ".": "./dist/index.mjs",
        "./server": "./dist/server.mjs",
        "./client": "./dist/client.mjs"
    },
    "type": "module",
    "scripts": {
        "build": "tsc || exit 0",
        "build:watch": "tsc -w --preserveWatchOutput",
        "dev": "pnpm build:watch"
    },
    "keywords": [
        "rpc",
        "window",
        "iframe",
        "postmessage",
        "web",
        "worker",
        "context",
        "browser"
    ],
    "contributors": [
        {
            "name": "Omran Jamal",
            "email": "omran@omranjamal.me",
            "url": "https://omranjamal.me"
        }
    ],
    "license": "MIT",
    "devDependencies": {
        "turbo": "1.10.13",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "evt": "2.5.7"
    },
    "repository": {
        "url": "https://github.com/open-fringecore/typed-postmessage-rpc",
        "type": "git"
    }
}
